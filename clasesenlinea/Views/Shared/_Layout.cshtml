<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Dibujo de proyecto mecanico</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")
    <script src="https://kit.fontawesome.com/db7440feab.js" crossorigin="anonymous"></script>

</head>
<body>
    <div class="navbar fondoazul2 navbar-fixed-top">
        <div>
            <div>
                <div class="container">
                    <img src="~/Content/logos/UASLP.png" width="300px" />
                    <img src="~/Content/logos/Escudo.png" width="300px" />
                </div>
                <div class=" textotitulo">
                    Dibujo de proyecto mecánico
                </div>
            </div>
            <div class="navbar-header">
               
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Calificaciones", "Index", "Home")</li>
                    <li>@Html.ActionLink("Temario", "About", "Home")</li>
                    <li>@Html.ActionLink("Información adicional", "Contact", "Home")</li>
                    <li>@Html.ActionLink("Contacto", "Contact", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="margenarriba">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET</p>
        </footer>
    </div>
    <script type="text/javascript" src="~/Scripts/funciones.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
